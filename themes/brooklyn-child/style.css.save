/*Header Google  Search*/
.header-google-search {
  visibility: hidden;
  overflow: hidden;
  height: 0;
  border-right: 1px solid #ccc;
  border-left: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
  position: fixed; right: 7%; float: right; padding: 1px; padding-right: 10px; width: 500px; background-color: #fff; z-index: 10000;
     -webkit-transition: all .5s ease;
    -moz-transition: all .5s ease;
    transition: all .5s ease;    
}
.display-google-search {
  visibility: visible;
  height: auto;
  padding: 5px; padding-right: 15px;
}

.mobile-google-search {
  display: none;
}

/*Media Queries*/
/* Phone & Small Tablets */
@media screen and (max-width: 767px) {
     .latestpost-modal-content {
          width: 80%;
     }
     .panel, .so-widget-ink-tabs{
          margin: 0 auto;
     }
     .dropdown-wrapper {
          margin-left: 0%; width: 100%; padding-bottom: 20px;
     }

     /*div.site-logo > a > img {
          height: auto !important;
          width: 100% !important;
          height: 80.5px !important;
          width: 280px !important;
     }

     div.site-logo > a{
          height: 80.5px !important;
          width: 280px !important;
     }

/*Header Google Search*/
     .header-google-search {
          display: none !important; 
     }
     .mobile-google-search{
          display: block;
     }
     .google-search-open{
          display: none;
     }
     .menu-item-search{
    	  display: none;
      }
}
/* Tablet Landscape */
@media screen and (min-width: 768px) and (max-width: 1024px) {
     .header-google-search {
          display: none !important; 
     }
     .mobile-google-search{
          visibility: visible;
     }
     .google-search-open{
          display: none;
     }
     .menu-item-search{
	  display: none;
     }
}

