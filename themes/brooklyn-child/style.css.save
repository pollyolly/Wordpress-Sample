/*Latest Post Modal*/
.latestpost-modal {
    animation:opac 0.8s;
    left: 0;
    top: 0;
    position: fixed;
    z-index: 99999;
    background-color: rgb(0,0,0); /* Fallback color */
    background-color: rgba(0,0,0,0.4); /* Black w/ opacity */
    width: 100%; 
    height: 100%
} @keyframes opac{from{opacity:0} to{opacity:1}}
.latestpost-modal-content {
    position: relative;
    background-color: white;
    margin: 5% auto;
    /*width: 40%;*/
    width: 700px;
    height: auto;
    overflow: auto;
    padding: 15px;
    border: 5px solid #12553c;
}
.latestpost-modal-header span{
    display: inline-block;
    font-size: 24px;
    font-weight: bold;
}
.latestpost-modal-header .close {
  float: right;
}
.latestpost-modal-header {
  padding: 2px 5px 25px;
}
.latestpost-modal-header .close{
  float: right;
  font-size: 28px;
  font-weight: bold;
  color: #ccc;
}
.latestpost-modal-header .close:hover, .search-modal-header .close:focus {
    color: #7A1013;
    text-decoration: none;
    cursor: pointer;
}
.latestpost-readmore a {
  color: #7A1013;
  font-weight: bold;
}
.latestpost-readmore a:hover {
  text-decoration: underline;
}
.latestpost-excerpt p {
  color: #6f6e6e;
}
/*Survey Form and Helpdesk*/
.ordinary-modal {
    display: none;
    animation:opac 0.8s;
    left: 0;
    top: 0;
    position: fixed;
    z-index: 99999;
    background-color: rgb(0,0,0); /* Fallback color */
    background-color: rgba(0,0,0,0.4); /* Black w/ opacity */
    width: 100%; 
    height: 100%
} @keyframes opac{from{opacity:0} to{opacity:1}}

.ordinary-modal-content {
    position: relative;
    background-color: white;
    margin: 5% auto;
    width: 80%;
    height: 80%;
    overflow: hidden;
}
.ordinary-modal-header .close {
  float: right;
}
.ordinary-modal-header {
  padding: 10px 20px 25px;
}
.ordinary-modal-header .close{
  float: right;
  font-size: 28px;
  font-weight: bold;
  color: #ccc;
}
.ordinary-modal-header .close:hover, .ordinary-modal-header .close:focus {
    color: #7A1013;
    text-decoration: none;
    cursor: pointer;
}
.ordinary-modal-content iframe {
  width: 100%;
  height: 100%;
  padding: 17px;
  padding-bottom: 50px;
}
/*Scroll to top*/
.myscroll-to-top {
    display: none;
    z-index: 99999;
    position: fixed;
    bottom: 20px;
    right: 20px;
    background: rgb(0, 0, 0);
    background: rgba(0, 0, 0, 0.4);
    width: 50px;
    height: 50px;
    text-decoration: none;
    -webkit-border-radius: 35px;
    -moz-border-radius: 35px;
    border-radius: 1px;
    -webkit-transition: all 0.3s linear;
    -moz-transition: all 0.3s ease;
    -ms-transition: all 0.3s ease;
    -o-transition: all 0.3s ease;
    transition: all 0.3s ease;
}
.myscroll-to-top span {
    color: #fff;
    margin: 0;
    position: relative;
    left: 16px;
    top: 13px;
    font-size: 19px;
    -webkit-transition: all 0.3s ease;
    -moz-transition: all 0.3s ease;
    -ms-transition: all 0.3s ease;
    -o-transition: all 0.3s ease;
    transition: all 0.3s ease;
}
.myscroll-to-top:hover {
    background: rgba(0, 0, 0, 0.9);
}
.myscroll-to-top:hover span {
    color: #fff;
    top: 5px;
}
/*Scroll to top*/
